            <th>
                @Html.DisplayNameFor(model => model.GeoJSON)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FireTypeClassification)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SmokeTypeClassification)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FireScoreClassification)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SmokeScoreClassification)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Latitude)
