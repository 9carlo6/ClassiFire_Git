                @Html.DisplayFor(modelItem => item.FileType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Geolocalization)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeoJSON)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FireTypeClassification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SmokeTypeClassification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FireScoreClassification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SmokeScoreClassification)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
